<Window x:Class="OpenCyralive.ocSchedule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenCyralive"
        mc:Ignorable="d"
        Title="作息时间" Height="450" Width="800" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Grid x:Name="SWindowContent" Margin="10,10,10,10">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <TextBlock x:Name="Schedule_time" Grid.Row="0" Grid.Column="0"  HorizontalAlignment="Left" Margin="0,5,0,0" TextWrapping="Wrap" Text="修改作息时间: " VerticalAlignment="Top" FontWeight="Bold" FontSize="14"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="1" Grid.Column="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="1" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="2" Grid.Column="0"  TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="2" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="3" Grid.Column="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="3" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="4" Grid.Column="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="4" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="5" Grid.Column="0"  TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="5" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="6" Grid.Column="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="6" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="7" Grid.Column="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="7" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="8" Grid.Column="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="8" Grid.Column="1" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="1" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="1" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="2" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="2" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="3" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="3" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="4" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="4" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="5" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="5" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="6" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="6" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="7" Grid.Column="3" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="7" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="8" Grid.Column="3"  TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="8" Grid.Column="4" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="1" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="1" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="2" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="2" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="3" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="3" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="4" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="4" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="5" Grid.Column="6"  TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="5" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="6" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="6" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="7" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="7" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <TextBlock HorizontalAlignment="Left" Grid.Row="8" Grid.Column="6" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Grid.Row="8" Grid.Column="7" VerticalAlignment="Top" Width="120"/>
            <Button x:Name="confirm_change" Grid.Row="9" Grid.Column="9" Content="确定" HorizontalAlignment="Left" VerticalAlignment="Top" Click="confirm_change_Click"  Height="20" Width="30"/>
            <Button x:Name="cancel_change" Grid.Row="10" Grid.Column="10" Content="取消" HorizontalAlignment="Left" VerticalAlignment="Top" Click="cancel_change_Click"  Height="20" Width="30"/>
        </Grid>
    </ScrollViewer>
</Window>
